@startuml
title FingerprintMIS8 - Level 1 DFD (Course Enrollment)

skinparam rectangle {
  BackgroundColor #f06292
  BorderColor black
  RoundCorner 15
  FontColor black
}

skinparam actor {
  BackgroundColor #4db6ac
  BorderColor black
  FontColor black
}

actor "Admin" as Admin #4db6ac
actor "Instructor" as Instructor #4db6ac
actor "Users" as Users #4db6ac

rectangle "2.1 Register Course" as RegisterCourse #f06292
rectangle "2.2 Enroll Student" as EnrollStudent #f06292
rectangle "2.3 Update Enrollment" as UpdateEnrollment #f06292

database "D5: Student_Course" as D5 #ffccff
database "D6: Courses" as D6 #ffccff
database "D4: Students" as D4 #ffccff

Admin -[#0000FF]-> RegisterCourse : Course Details
RegisterCourse -[#0000FF]-> D6 : Store Course Data

Instructor -[#0000FF]-> EnrollStudent : Enrollment Requests
EnrollStudent -[#0000FF]-> D5 : Store Enrollment Data
EnrollStudent -[#0000FF]-> D4 : Verify Student Data

Instructor -[#0000FF]-> UpdateEnrollment : Update Requests
UpdateEnrollment -[#0000FF]-> D5 : Update Enrollment Data

@enduml
